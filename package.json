{
  "name": "bmultisig",
  "version": "1.1.0-beta",
  "description": "Bcoin wallet plugin for multi signature transaction proposals",
  "repository": "git://github.com/bcoin-org/bmultisig.git",
  "homepage": "https://github.com/bcoin-org/bmultisig",
  "main": "./lib/plugin",
  "bin": {
    "bmultisig": "./bin/bmultisig"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint lib/ test/ || exit 0",
    "lint-ci": "eslint lib/ test/",
    "test": "bmocha --reporter spec test/*-test.js",
    "test-ci": "istanbul cover node_modules/.bin/bmocha --report lcovonly -- --reporter spec test/*.js",
    "docs": "jsdoc -c jsdoc.json",
    "clean": "rm -rf coverage && rm -rf docs"
  },
  "keywords": [
    "bcoin",
    "bitcoin",
    "blockchain",
    "wallet",
    "multisig"
  ],
  "author": "bcoin",
  "license": "MIT",
  "dependencies": {
    "bclient": "^0.1.7",
    "bcoin": "~1.0.2",
    "bcrypto": "^3.1.0",
    "bdb": "^1.1.7",
    "bevent": "^0.1.5",
    "blgr": "^0.1.7",
    "bmultisig-client": "0.0.5",
    "bmutex": "^0.1.6",
    "bsert": "0.0.10",
    "bstring": "^0.3.8",
    "bufio": "^1.0.6",
    "bval": "^0.1.6",
    "bweb": "^0.1.8"
  },
  "devDependencies": {
    "bmocha": "^2.1.0"
  },
  "browser": {
    "./lib/bmultisig": "./lib/bmultisig-browser",
    "./lib/utils/inspect": "./lib/utils/inspect-browser"
  }
}
